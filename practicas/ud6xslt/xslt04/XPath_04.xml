<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="text/xsl" href="xpath_04.xsl"?>
<!DOCTYPE actividades [
<!ELEMENT actividades (actividad+)>
<!ELEMENT actividad (codigo, titulo, fecha_inicio, sesiones)>
<!ELEMENT codigo (#PCDATA)>
<!ELEMENT titulo (#PCDATA)>
<!ELEMENT fecha_inicio (#PCDATA)>
<!ELEMENT sesiones (minutos_sesion, sesiones_semana)>
<!ELEMENT minutos_sesion (#PCDATA)>
<!ELEMENT sesiones_semana (#PCDATA)>
<!ATTLIST actividad nivel CDATA #REQUIRED>
<!ATTLIST sesiones intensidad CDATA #REQUIRED>
]>
<actividades xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:noNamespaceSchemaLocation= "xpath_04.xsd">
	<actividad nivel="infantil">
		<codigo>A01</codigo>
